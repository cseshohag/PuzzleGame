<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/shohag"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/shohag"
        android:layout_marginTop="59dp"
        android:fontFamily="sans-serif-condensed"
        android:text="@string/programmer_info"
        android:textColor="@android:color/background_light"
        android:textAllCaps="true"
        android:textSize="24sp"
        android:textStyle="bold"
        android:visibility="visible" />

    <ImageView
        android:id="@+id/shohag"
        android:layout_width="105dp"
        android:layout_height="113dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="18dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="98dp"
        android:src="@drawable/shohag" />

    <ImageView
        android:id="@+id/sadiya"
        android:layout_width="108dp"
        android:layout_height="113dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/shohag"
        android:layout_marginEnd="119dp"
        android:src="@drawable/sadiya" />

    <ImageView
        android:id="@+id/kajol"
        android:layout_width="82dp"
        android:layout_height="113dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/shohag"
        android:layout_marginEnd="14dp"
        android:layout_marginRight="14dp"
        android:src="@drawable/kajol" />

    <TextView
        android:id="@+id/shohag_intro1"
        android:layout_width="108dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/shohag_intro3"
        android:layout_alignLeft="@+id/shohag"
        android:layout_alignStart="@+id/shohag"
        android:fontFamily="sans-serif"
        android:text="Md. Solaiman Shoahg"
        android:textAllCaps="false"
        android:textColor="@android:color/background_light"
        android:textColorLink="@android:color/background_light"
        android:textSize="10dp"
        android:textStyle="bold"
        android:typeface="normal"
        android:visibility="visible" />

    <TextView
        android:id="@+id/shohag_intro2"
        android:layout_width="108dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/textView"
        android:layout_marginBottom="113dp"
        android:fontFamily="sans-serif"
        android:textColor="@android:color/background_light"
        android:text="B.SC. in CSE"
        android:textAllCaps="false"
        android:textSize="10dp"
        android:textStyle="bold"
        android:typeface="normal"
        android:visibility="visible" />

    <TextView
        android:id="@+id/shohag_intro3"
        android:layout_width="108dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/shohag_intro2"
        android:layout_alignLeft="@+id/shohag"
        android:layout_alignStart="@+id/shohag"
        android:fontFamily="sans-serif"
        android:textColor="@android:color/background_light"
        android:text="Team Leader"
        android:textAllCaps="false"
        android:textSize="10dp"
        android:textStyle="bold"
        android:typeface="normal"
        android:visibility="visible" />


    <TextView
        android:id="@+id/sadiya_intro1"
        android:layout_width="108dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/shohag_intro3"
        android:layout_alignLeft="@+id/sadiya"
        android:layout_alignStart="@+id/sadiya"
        android:fontFamily="sans-serif"
        android:textColor="@android:color/background_light"
        android:text="Sadiya Newaz"
        android:textAllCaps="false"
        android:textSize="10dp"
        android:textStyle="bold"
        android:typeface="normal"
        android:visibility="visible" />

    <TextView
        android:id="@+id/sadiya_intro2"
        android:layout_width="108dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/sadiya"
        android:layout_alignStart="@+id/sadiya"
        android:layout_alignTop="@+id/shohag_intro2"
        android:fontFamily="sans-serif"
        android:textColor="@android:color/background_light"
        android:text="B.SC. in CSE"
        android:textAllCaps="false"
        android:textSize="10dp"
        android:textStyle="bold"
        android:typeface="normal"
        android:visibility="visible" />

    <TextView
        android:id="@+id/sadiya_intro3"
        android:layout_width="108dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/sadiya_intro2"
        android:layout_alignLeft="@+id/sadiya"
        android:layout_alignStart="@+id/sadiya"
        android:fontFamily="sans-serif"
        android:textColor="@android:color/background_light"
        android:text="Team Member"
        android:textAllCaps="false"
        android:textSize="10dp"
        android:textStyle="bold"
        android:typeface="normal"
        android:visibility="visible" />

    <TextView
        android:id="@+id/kajol_intro1"
        android:layout_width="108dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/kajol_intro3"
        android:layout_alignLeft="@+id/kajol"
        android:layout_alignStart="@+id/kajol"
        android:fontFamily="sans-serif"
        android:textColor="@android:color/background_light"
        android:text="Md. Kaisar Hamid"
        android:textAllCaps="false"
        android:textSize="10dp"
        android:textStyle="bold"
        android:typeface="normal"
        android:visibility="visible" />

    <TextView
        android:id="@+id/kajol_intro2"
        android:layout_width="96dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/shohag_intro2"
        android:fontFamily="sans-serif"
        android:textColor="@android:color/background_light"
        android:text="B.SC. in CSE"
        android:textAllCaps="false"
        android:textSize="10dp"
        android:textStyle="bold"
        android:typeface="normal"
        android:visibility="visible" />

    <TextView
        android:id="@+id/kajol_intro3"
        android:layout_width="108dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/kajol_intro2"
        android:layout_alignLeft="@+id/kajol"
        android:layout_alignStart="@+id/kajol"
        android:fontFamily="sans-serif"
        android:textColor="@android:color/background_light"
        android:text="Team member"
        android:textAllCaps="false"
        android:textSize="10dp"
        android:textStyle="bold"
        android:typeface="normal"
        android:visibility="visible" />


</RelativeLayout>